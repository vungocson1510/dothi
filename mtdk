#include<iostream>
using namespace std;
# define max 11

int matrix[max + 1][max + 1];
//khoi tao ma tran
void taomatrix() {
	for (int i = 1; i <= max; i++) {
		for (int j = 1; j <= max; j++)
			matrix[i][j] = 0;
	}
}
//xac dinh cai canh
void themcanh(int i, int j) {
	matrix[i][j] = 1;
	matrix[j][i] = 1;
}
//in ma tran
void inmatrix() {
	cout << " in ma tran " << endl;
	for (int i = 1; i <= max; i++){
		for (int j = 1; j <= max; j++)
			cout << matrix[i][j] << " ";
		cout << endl;
	}
}
int main() {
	taomatrix();
	themcanh(1, 2);
	themcanh(1, 6);
	themcanh(1, 7);
	themcanh(1, 8);
	themcanh(1, 9);
	themcanh(1, 10);
	themcanh(2, 3);
	themcanh(2, 11);
	themcanh(3, 4);
	themcanh(4, 5);
	themcanh(4, 6);
	themcanh(5, 6);
	themcanh(10, 11);
	inmatrix();
	return 0;
}
